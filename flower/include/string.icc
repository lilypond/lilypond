#ifndef STRING_ICC
#define STRING_ICC


INLINE 
char &
String::operator [] (int n)
{
  return (char &) strh_[n]; 
}

INLINE 
char 
String::operator [] (int n) const
{
  return strh_[n]; 
}

INLINE 
String::String () 
{
}

INLINE
String::String (char const* source)
{   
  assert (source);    
  strh_ = source;    
}


#endif /* STRING_ICC */
